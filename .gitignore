FIBONACCI EXERCISE

#include <iostream>
using namespace std;
int fibo (int n);
 main()
{
int n;
cout<< "Nhap vao gia tri n: ";
cin>>n;
cout<< fibo(n);
}
int fibo (int n)
{
 int f0=1,f1=1,f2;
 for (int i=2;i<=n;i++)
 {
     f2=f1+f0;
     f0=f1;
     f1=f2;
 }
 return f2;
}
